extends layout

block content
  section
    .center
      img.logo(src="images/one-web.svg")
      h2 JSON Template Finder
      |
      form(action='/search', method='post')
        input#search(type='text', placeholder='e.g. A887B21D-7067-42F0-924C-5433BFE46B7C' name='search' autofocus)
        button(type='submit')
        p.template-name #{ name }
        p.template-json #{ templateId }
      dl
        dd Total pages:
        span #{ numberPages }
        div.divider
          img(src='data:image/svg+xml;utf8;base64,')
        dd Stylesheet: 
        span #{ style }

        div.divider
          img(src='data:image/svg+xml;utf8;base64,')
        - var pages = pagesArray;
        each page in pages.length ? pages: ['No page associated']
          dd.page= page["name"] 
          span.pageid= page["id"]
